{
  "name": "keyword-brunch",
  "version": "1.7.5",
  "description": "Adds keyword processing support to brunch.io application packager.",
  "author": "Huafu Gandon <huafu.gandon@gmail.com>",
  "homepage": "https://github.com/huafu/keyword-brunch",
  "repository": {
    "type": "git",
    "url": "git@github.com:huafu/keyword-brunch.git"
  },
  "main": "./lib/index",
  "scripts": {
    "prepublish": "rm -rf ./lib && ./node_modules/coffee-script/bin/coffee -o lib/ src/",
    "postinstall": "rm -rf ./lib && ./node_modules/coffee-script/bin/coffee -o lib/ src/",
    "test": "./node_modules/.bin/mocha --reporter spec --compilers coffee:coffee-script/register --require coffee-script/register --require test/common --colors"
  },
  "engines": {
    "node": "~0.6.10 || 0.8 || 0.9 || 0.10"
  },
  "dependencies": {
    "coffee-script": ">= 1.6",
    "regexp-quote": "0.0.0"
  },
  "devDependencies": {
    "chai": "1.2.0",
    "mkdirp": "^0.5.0",
    "mocha": "1.3.2",
    "ncp": "^0.5.1",
    "rmdir": "^1.0.4",
    "sinon": "^1.10.2",
    "sinon-chai": "^2.5.0"
  },
  "bugs": {
    "url": "https://github.com/huafu/keyword-brunch/issues"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "brunch",
    "plugin",
    "keyword"
  ],
  "license": "MIT"
}
